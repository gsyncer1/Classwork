public class Lab1
{

  public static void main( String[] args )
  {
    World w = new World();

    // Creates a turtle in World w at (x,y) (100,50)
    Turtle maria = new Turtle( 100, 50, w );  

    // Make a U shape
    Turtle.forward( 55 );
    Turtle.turn( 90 );
    Turtle.forward( 30 );
    Turtle.turn( 90 );
    Turtle.forward( 55 );
  }

} // end class Lab1

